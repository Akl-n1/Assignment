@model Employee
@{
	ViewData["Title"] = "Details";
}

<h1 style="color:@ViewBag.Color">@Model.Name</h1>
<hr />
<h2>@ViewData["Message"]</h2>
<hr />
<h2>@ViewBag.Message</h2>
<hr />

<h2>@(int.Parse(ViewData["Temp"].ToString()) - 10)</h2>
<hr />
<h2>@(ViewBag.Temp - 10)</h2>
<hr />
<select>
	@foreach (var item in (List<string>)ViewData["branch"])
	{
		<option>
			<h1>@item</h1>
		</option>
	}
</select>
<hr />
<br />
<select>
	@foreach (var item in ViewBag.branch)
	{
		<option>
			<h1>@item</h1>
		</option>
	}
</select>